
public class TestaLacos2 {

	public static void main(String[] args) {
		for (int i = 0; i < 10; i++) {
			for (int j = 0; j < 10; j++) {
				if(j > i) {
					break;
				}
				System.out.print("*");
			}
			System.out.println();
		}
		
		for (int i = 1; i <= 100; i++) {
			if(i%3 == 0) {
				System.out.println(i + " é multiplo de 3");
			}
		}
	}
}
