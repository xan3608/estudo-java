
public class TesteFuncionario {

	public static void main(String[] args) {
		Funcionario alexandre = new Funcionario();
		alexandre.setNome("Aexandre");
		alexandre.setCpf("145.654.785-25");
		alexandre.setSalario(1500.56);
		
		System.out.println(alexandre.getNome());
		System.out.println(alexandre.getBonificacao());
	}
}
